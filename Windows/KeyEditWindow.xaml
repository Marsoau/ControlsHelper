<Window x:Class="ControlsHelper.Windows.KeyEditWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ControlsHelper.Windows"
        mc:Ignorable="d"
        Title="KeyEditWindow"
        SizeToContent="Height"
        Width="200">
    <StackPanel Margin="4">
        <TextBlock Text="Edit Key Title" HorizontalAlignment="Center"/>
        <TextBox x:Name="TextBox_Title" Margin="0,4"/>
        <Button x:Name="Button_Confirm" Content="Confirm" Padding="2,0" Click="Button_Confirm_Click"/>
    </StackPanel>
</Window>
